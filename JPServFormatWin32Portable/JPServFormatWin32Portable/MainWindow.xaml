<Window x:Class="JPServFormatWin32Portable.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JPServFormatWin32Portable"
        mc:Ignorable="d" ResizeMode="CanMinimize"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Label x:Name="ServFormatterLabel" Content="Server Formatter" Margin="75,0,67,242" FontSize="48" FontWeight="Normal" HorizontalContentAlignment="Center" Height="68" VerticalAlignment="Bottom"/>
        <Label x:Name="label" Content="Type the ip here (without ws://)" HorizontalAlignment="Left" Height="25" Margin="146,78,0,0" VerticalAlignment="Top" Width="219" HorizontalContentAlignment="Center" FontSize="10.667"/>
        <TextBox x:Name="results" HorizontalAlignment="Left" Height="129" Margin="75,153,0,0" TextWrapping="Wrap" Text="Results here" VerticalAlignment="Top" Width="361" FontSize="14.667"/>
        <Grid HorizontalAlignment="Left" Height="45" Margin="75,103,0,0" VerticalAlignment="Top" Width="361">
            <TextBox x:Name="ip"  HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" Text="1.1.1.1:443" VerticalAlignment="Top" Width="140"/>
            <GridSplitter x:Name="gridSplitter" HorizontalAlignment="Left" Height="45" Margin="175,0,0,0" VerticalAlignment="Top" Width="5"/>
            <Button x:Name="getResult" Content="Get the code" HorizontalAlignment="Left" Margin="206,10,0,0" VerticalAlignment="Top" Width="130" Click="button_Click"/>
        </Grid>
        <Label x:Name="label1" Content="By jeuxjeux20" HorizontalAlignment="Left" Height="25" Margin="0,297,0,0" VerticalAlignment="Top" Width="82"/>

    </Grid>
</Window>
